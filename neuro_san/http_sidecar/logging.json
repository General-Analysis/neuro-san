{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "structured": {
            "format": "{\"level\": \"%(levelname)s\", \"message\": \"%(message)s\", \"Timestamp\": \"%(asctime)s\" \"file\": \"%(filename)s\"}"
        }
    },
    "handlers": {
        "null": {
            "class": "logging.NullHandler",
            "level": "INFO"
        },
        "console": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "structured",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "my_module": {
            "level": "INFO",
            "handlers": [
                "console"
            ],
            "propagate": "no"
        }
    },
    "root": {
        "level": "INFO",
        "handlers": [
            "console"
        ]
    }
}
